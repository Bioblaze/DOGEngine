{
  "name": "dog-engine",
  "maintainers": "Randolph William Aarseth II <randolph@divine.games>",
  "supports": "windows",
  "dependencies": [
    "cereal",
    "entt",
    "fmt",
    "glew",
    "glfw3",
    "glm",
    {
      "name": "imgui",
      "features": [
        "opengl3-binding",
        "sdl2-binding",
        "sdl2-renderer-binding"
      ]
    },
    "opengl",
    "sdl2",
    {
      "name": "sdl2-image",
      "features": [
        "libjpeg-turbo"
      ]
    },
    {
      "name": "sdl2-mixer",
      "features": [
        "mpg123"
      ]
    },
    "sdl2-net"
  ]
}
